	rdfs:label "Ontology Forward Engineering Activity NeOn"@en ;
	rdfs:seeAlso obt:publication_NeOn531 .

obt:activity_A19_Plan rdf:type p-plan:Plan .

obt:activity_A1_Plan rdf:type p-plan:Plan .

obt:activity_A2 rdf:type obtenv:Activity ;
